<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.practice.actionbardemo"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.practice.actionbardemo.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.practice.actionbardemo.HelpActivity"
            android:label="@string/app_name" 
            android:parentActivityName="com.practice.actionbardemo.MainActivity"
            >
           
             </activity>
         <activity
            android:name="com.practice.actionbardemo.LightActivity"
            android:label="@string/app_name" 
            android:parentActivityName="com.practice.actionbardemo.MainActivity"
            android:theme="@android:style/Theme.Holo.Light"
            >
            </activity>
            <activity
            android:name="com.practice.actionbardemo.OverlayActivity"
            android:label="@string/app_name" 
            android:parentActivityName="com.practice.actionbardemo.MainActivity"
            android:theme="@style/overlayTheme"
            >
            </activity>
    </application>

</manifest>
